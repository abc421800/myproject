  <select id="selectListByOrderCode" resultMap="BaseResultMap" parameterType="string" >
   		SELECT w.*,e.name as 'enterpriseName',e.id as 'enterpriseId' FROM yaohao_winbid w left JOIN cost_enterprise e  ON w.enterprise_code= e.`code`  where  w.order_code=#{orderCode}
