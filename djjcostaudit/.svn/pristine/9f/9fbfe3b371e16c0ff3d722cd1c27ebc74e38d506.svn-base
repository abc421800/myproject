@charset "UTF-8";
*{
    margin:0;
    padding:0;
}
.panel-fit, .panel-fit body{padding:15px;}
.layout-split-west {
    border-right: 14px solid transparent;
    background: transparent;
}
.panel {
    -moz-box-shadow: none;
    -webkit-box-shadow: none;
    -o-box-shadow: none;
    -ms-box-shadow: none;
    box-shadow: none;
}
.panel-title{
    padding-left:20px;
    color:#283747;
    font-size: 12px;
}
.panel-header {
    border-color: #ced2d6;
    padding:14px 10px 14px;
    background:#dde3e8;
}
.tabs-header{
    padding-top: 5px;
}
#center{
    background-color: #f4f8fb;
}
.panel-body {
    border-color: #ced2d6;
    overflow: hidden;
}
.wrap{
    border: none;
    background: #f4f8fb;
}
.pagination{
    border-radius: 0;
}
.tabs li.tabs-selected a.tabs-inner {
    border-bottom: 1px solid #fff;
    background: #fff;
}
body{
    /*min-width: 1080px;*/
}
a{
    text-decoration: none;
}
.content .top{
    height: 36px;
}
.content .top .title{
    font-size: 18px;
    line-height: 36px;
    text-align: center;
    font-weight: bold;
    color: #fff;
    background-color: #2a3f54;
}
.content .middle{
    height: 146px;
    padding: 6px;
}
.content .middle .title{

    font-size: 16px;
    color: #326699;
    text-align: center;
    font-weight: bold;
}
.content .middle .wrap1{
    padding-top: 20px;
    padding-bottom: 24px;
}
.content .middle .wrap2{
    padding-top: 12px;
    padding-bottom: 12px;
}
.content .middle .wrap3{
    padding-top: 12px;
    padding-bottom: 12px;
}
.content .middle .wrap4{
    padding-top: 20px;
    padding-bottom: 24px;
}
.content .middle .wrap5{
    padding-top: 30px;
    padding-bottom: 24px;
}
.content .middle .wrap6{
    padding-top: 40px;
    padding-bottom: 24px;
}
.content .middle .phase{

    line-height: 20px;
    color: #878787;
    font-size: 14px;


}
.content .bottom{
    height: 40px;

}
.content .middle .phase>span{
    color: #2a3e54;
}
.content .bottom>a{
    display: block;
    text-decoration: none;
    line-height: 40px;
    font-size: 30px;
    color: #c7c8c8;
    text-align: center;
    font-weight: bold;
}
/* menu */
dl,dd{
    margin:0;
}
.left-menu dl dd{
    height: 56px;
    border-bottom: 1px solid #ddd;
 }
.left-menu dl dd span{
    display: inline-block;
    line-height: 30px;
    height: 30px;
    width:94px;
    color:#929ba5;
    font-size: 14px;
    border: 1px solid #e2e8ee;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    text-align: center;
    margin:12px 30px;
    cursor: pointer;
    padding:0 12px;
}
.left-menu dl dd.active span,.left-menu dl dd span:hover{
    -webkit-box-shadow: 0px 4px 4px rgba(201,205,209,0.58);
    -moz-box-shadow: 0px 4px 4px rgba(201,205,209,0.58);
    box-shadow: 0px 4px 4px rgba(201,205,209,0.58);
    color:#283747;
    font-weight: bold;
}
.tabs li.tabs-selected a.tabs-inner {
    border-bottom: 1px solid #f4f8fb;
    background: #f4f8fb;
}

