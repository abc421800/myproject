//某年某月有几天
function getDaysInYearMonth(year, month){
	if(year=="" || month==""){
		var curDate = new Date();
		return curDate.getDate();
	}else{
		var curDate = new Date(year+"-"+month);
		/* 获取当前月份 */
		var curMonth = curDate.getMonth();
		/* 生成实际的月份: 由于curMonth会比实际月份小1, 故需加1 */
		curDate.setMonth(curMonth + 1);
		/* 将日期设置为0 */
		curDate.setDate(0);
		/* 返回当月的天数 */
		return curDate.getDate();
	}
}

