  <select id="selectListByBidflag" resultMap="BaseResultMap" parameterType="java.lang.String" >
  		SELECT * from  yaohao_candidate c where c.bid_flag = #{bidFlag}
  </select>
  <select id="selectListByMap" resultMap="BaseResultMap" parameterType="map" >
		
		SELECT IFNULL(a.winNum,0) as 'winNum',m.* FROM (
		SELECT e.id as 'enterpriseId', a.assess_result as 'assessResult',e.`name` as 'enterpriseName',e.simple_name as 'simpleName',c.*,a.`year` 
		FROM yaohao_candidate c 
		INNER JOIN cost_enterprise e ON c.enterprise_code=e.`code` 
		LEFT JOIN yaohao_assess a ON a.enterprise_code=c.enterprise_code AND a.`year`=c.yaohao_year
     	<where>
     		<if test="id != null and id !=''" >
     			and c.id = #{id}
   			</if>
   			<if test="enterpriseName != null and enterpriseName !=''" >
     			and e.name like concat('%',#{enterpriseName},'%')
   			</if>
   			<if test="yaohaoGrade != null and yaohaoGrade !=''" >
     			and c.yaohao_grade = #{yaohaoGrade}
   			</if>
   			<if test="yaohaoYear != null and yaohaoYear !=''" >
     			and c.yaohao_year = #{yaohaoYear}
   			</if>
   			<if test="yearAssess != null and yearAssess !=''" >
   				<choose>
   					<when test="yearAssess=='未评定'">
   						and a.assess_result is null
   					</when>
   					<otherwise>
		     			and a.assess_result = #{yearAssess}
   					</otherwise>
   				</choose>
   			</if>
   			<if test="roundNum != null and roundNum !=''" >
     			and c.round_num = #{roundNum}
   			</if>
   			<if test="bidFlagNull != null and bidFlagNull !=''" >
     			and  (c.bid_flag IS NULL or c.bid_flag ='')
   			</if>
   			<if test="noEntId != null and noEntId !=''" >
     			AND NOT FIND_IN_SET(e.id,#{noEntId})
   			</if>
        </where>
        	ORDER BY e.name
        )m LEFT JOIN(
			SELECT COUNT(w.id) as 'winNum',enterprise_code FROM yaohao_winbid w LEFT JOIN yaohao_order o ON o.code=w.order_code
			<where>
				<if test="yaohaoGrade != null and yaohaoGrade !=''" >
	     			and o.yaohao_grade = #{yaohaoGrade}
	   			</if>
			</where>
			 GROUP BY w.enterprise_code
		)a ON a.enterprise_code=m.enterprise_code
     <if test="isPage==true">
    		limit #{curPage},#{pageSize}
    </if>
  </select>
